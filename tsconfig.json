{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "strict": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "baseUrl": "src",
    "paths": {
      "styles/*": ["styles/*"],
      "graph/*": ["graph/*"]
    }
  },
  "include": [
    "src"
  ]
}
