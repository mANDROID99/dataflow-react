{
    "compilerOptions": {
      "target": "es6",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true,
      "declaration": true,
      "downlevelIteration": true,
      "strict": true,
      "sourceMap": true,
      "forceConsistentCasingInFileNames": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "react"
    }
  }
  